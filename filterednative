   ,Para [Str "If",Str " ",Code ("",[],[]) "self",Str " ",Str "is",Str " ",Code ("",[],[]) "Some(s)",Str " ",Str "and",Str " ",Code ("",[],[]) "other",Str " ",Str "is",Str " ",Code ("",[],[]) "Some(o)",Str ",",Str " ",Str "this",Str " ",Str "method",Str " ",Str "returns",Str " ",Code ("",[],[]) "Some(f(s, o))",Str ".",Str "Otherwise,",Str " ",Code ("",[],[]) "None",Str " ",Str "is",Str " ",Str "returned."]
